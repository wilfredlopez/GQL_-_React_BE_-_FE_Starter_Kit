{
  "name": "qqlreact-starter",
  "version": "1.0.0",
  "description": "Starter Kit for QrapqhQL Backend and React Frontend",
  "main": "dist/index.js",
  "keywords": [
    "Typescript",
    "Graphql",
    "React",
    "MERN"
  ],
  "repository": {
    "url": "https://github.com/wilfredlopez/GQL_-_React_BE_-_FE_Starter_Kit"
  },
  "author": "Wilfred Lopez",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^2.9.3",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.3",
    "cloudinary": "^1.17.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dataloader": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.5.4",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.0.8",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "ts-node": "3.3.0",
    "type-graphql": "^0.17.6",
    "typeorm": "0.2.18"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.1",
    "@types/gravatar": "^1.8.0",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/mongodb": "^3.3.1",
    "@types/node": "^8.0.29",
    "@types/nodemailer": "^6.2.1",
    "@types/yup": "^0.26.24",
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.2"
  },
  "engines": {
    "node": "10.x"
  },
  "cacheDirectories": [
    "node_modules",
    "frontend/node_modules"
  ],
  "scripts": {
    "build": "rimraf dist/ && tsc",
    "start": "node dist/index.js",
    "build-client": "cd frontend/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build",
    "heroku-postbuild": "npm run build && npm run build-client",
    "dev": "ts-node-dev server/index.ts",
    "typeorm:create": "ts-node run typeorm migration:create -n"
  }
}
